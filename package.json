{
  "name": "outdoor-lovers",
  "version": "1.0.0",
  "description": "Web for outdoor enthusiasts",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font/flaticon.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/annabala/outdoor-lovers-web-project.git"
  },
  "author": "Anna",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/annabala/outdoor-lovers-web-project/issues"
  },
  "homepage": "https://github.com/annabala/outdoor-lovers-web-project#readme",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "concat": "^1.0.3",
    "node-sass": "^4.7.2",
    "postcss-cli": "^5.0.0"
  }
}
